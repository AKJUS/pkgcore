#!/bin/bash
# Copyright 1999-2005 Gentoo Foundation
# Distributed under the terms of the GNU General Public License v2
# $Id: newdoc 1912 2005-08-25 03:54:42Z ferringb $

if [ -z "${T}" ] || [ -z "${2}" ] ; then
	echo "newdoc: Nothing defined to do" 1>&2
	exit 1
fi

rm -rf "${T}/${2}"
cp "${1}" "${T}/${2}"
exec dodoc "${T}/${2}"
